#! /usr/bin/env python3

# Write your code here

def main():
    # 获取用户输入的整数R和C
    R, C = map(int, input().split())

    # 计算总的数字个数
    total_numbers = R * C

    # 循环遍历每行
    for i in range(R):
        # 初始化行字符串
        row_str = ""

        # 循环遍历每列
        for j in range(C):
            # 计算当前数字
            current_number = j * R + i + 1
            row_str += f"{current_number} "

        # 打印当前行字符串
        print(row_str.strip())

if __name__ == "__main__":
    main()


